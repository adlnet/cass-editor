{
  "name": "cass-editor",
  "version": "1.2.28",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port=8082 --fix",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "buildDev": "vue-cli-service build --mode development",
    "serveDev": "vue-cli-service serve --port=8082 --mode development",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "/theme.scss",
  "dependencies": {
    "bulma": "^0.9.0",
    "bulma-checkradio": "^1.1.1",
    "bulma-switch": "^2.0.0",
    "cassproject": "^2.11.7",
    "core-js": "^2.6.5",
    "dateformat": "^3.0.3",
    "file-saver": "^2.0.2",
    "jsonld": "^1.8.1",
    "pure-uuid": "^1.6.0",
    "v-clipboard": "^2.2.2",
    "vue": "^2.6.10",
    "vue-infinite-scroll": "^2.0.2",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.0.3",
    "vue-scrollto": "^2.17.1",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.3.0",
    "node-sass": "^4.13.1",
    "sass": "^1.23.2",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
